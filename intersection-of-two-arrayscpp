class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
    vector<int>ans;
    unordered_set<int>st;
    int n1 = nums1.size(), n2 = nums2.size();

    for(int i=0;i<n1;i++)
    st.insert(nums1[i]);

    for(int i=0;i<n2;i++)
    {
        if(st.find(nums2[i])!=st.end())
        {
            ans.push_back(nums2[i]);
            st.erase(nums2[i]);
        }
    }

    return ans;
        
    }
};