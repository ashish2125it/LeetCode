class Solution {
public:
    int areaOfMaxDiagonal(vector<vector<int>>& d) {
       int di = 0, ans = 0;
    for (int i = 0; i < d.size(); i++) {
        int newd = (d[i][0] * d[i][0] + d[i][1] * d[i][1]);
        if (newd > di) {
            di = newd;
            ans = d[i][0] * d[i][1];
        } else if (newd == di) {
            ans = std::max(ans, d[i][0] * d[i][1]);
        }
    }
    return ans;
    }
};